{
    "name": "@henningway/blueprint",
    "version": "0.1.9",
    "description": "Declarative object conversion for JavaScript.",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development rollup -c",
        "watch": "cross-env NODE_ENV=development rollup -c -w",
        "build": "cross-env NODE_ENV=production rollup -c",
        "test": "jest",
        "test:build": "npm run build && npm run test",
        "prettier": "prettier --write .",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/henningway/blueprint.git"
    },
    "author": "Henning Schindler <mail@henningschindler.de>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/henningway/blueprint/issues"
    },
    "homepage": "https://github.com/henningway/blueprint#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^13.1.3",
        "cross-env": "^7.0.3",
        "husky": "^5.1.2",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "rollup": "^2.40.0",
        "rollup-plugin-filesize": "^9.1.1",
        "rollup-plugin-terser": "^7.0.2"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none",
        "vueIndentScriptAndStyle": true
    }
}
